<div class="genre_search">
  <teble class="genre_table">
    <tr>
      <th>ジャンル検索</th>
    </tr>

      <% genres.each do |genre| %>
        <tr>
          <td>
          <%= link_to genre.name, items_path(genre_id: genre), method: :get %>
          </td>
        </tr>
      <% end %>
  </teble>
</div>