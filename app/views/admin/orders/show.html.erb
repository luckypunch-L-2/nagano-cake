<div class="container p-3">
  <div class="row">
  
  <div class="col-md-12">
    <h4>注文履歴詳細</h4>
    <table class="text-left">
      <tr>
        <th><strong>購入者</strong></th>
        <th><%= @order.customer.last_name + @order.customer.first_name %></th>
      </tr>
      <tr>
        <th><strong>注文日</strong></th>
        <th><%= @order.customer.created_at.strftime('%Y/%m/%d') %></th>
      </tr>
    </table>


